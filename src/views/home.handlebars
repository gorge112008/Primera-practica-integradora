<div class="main__container--side col s1"></div>
<div class="main__container__dinamic col s10">
  <div class="static__tittle row"><h4>*Productos Disponibles:</h4></div>
  <div class="static__container row">
    <div class="container__grid col m12">
      {{#each response}}
        <div class="container__grid__card">
          <div class="card">
            <div class="card-header"><h5
                class="card-title"
              >{{tittle}}</h5></div>
            <img class="card-img-top" src={{thumbnail}} alt="Card image cap" />
            <div class="card-body">
              <b class="card-text--description">{{description}}</b>
              <p class="card-text--price">S/.{{price}}</p>
              <b class="card-text--code">Code: <b class="code">{{code}}</b></b>
            </div>
          </div>
        </div>
      {{/each}}
    </div>
  </div>
</div>
<div class="main__container--side col s1"></div>
<div id="return"><a href="/" type="button"><i
      class="fas fa-light fa-reply-all"
    ></i><b>RETURN</b></a></div>
<script
  src="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.24/dist/sweetalert2.all.min.js"
></script>
<script
  src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"
></script>
<script src="/js/index.js"></script>